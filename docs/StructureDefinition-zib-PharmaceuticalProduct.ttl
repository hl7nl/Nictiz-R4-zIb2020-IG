  fhir:differential [
     fhir:element ( [
       fhir:id [ fhir:v "Medication" ] ;
       fhir:path [ fhir:v "Medication" ] ;
       fhir:short [ fhir:v "PharmaceuticalProduct" ] ;
       fhir:alias ( [ fhir:v "FarmaceutischProduct" ] ) ;
       fhir:mapping ( [
         fhir:identity [ fhir:v "zib-pharmaceuticalproduct-v2.1.2-2020EN" ] ;
