        "expression" : "start.hasValue().not() or end.hasValue().not() or (start <= end)",
        "xpath" : "not(exists(f:start/@value)) or not(exists(f:end/@value)) or (xs:dateTime(f:start/@value) <= xs:dateTime(f:end/@value))",
